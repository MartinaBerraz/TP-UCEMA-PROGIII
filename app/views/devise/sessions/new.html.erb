<section class="cont-img">
  <img src="<%= asset_path 'log.jpg' %>"> <!-- Es un comando de ruby -->
</section>
<div class="wrapper">
  <div class="loginbox">
<h1>Log in</h1>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email, class: "lab" %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "box" %>
  </div>

  <div class="field">
    <%= f.label :password, class: "lab" %><br />
    <%= f.password_field :password, autocomplete: "current-password", class: "box" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in", class: "sub" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
</div>
</div>