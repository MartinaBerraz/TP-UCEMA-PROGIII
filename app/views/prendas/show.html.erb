<body>
<nav class=" navbar navbar-expand-lg navbar-dark bg-dark">
  <h4 class="title">
    <a class="navbar-brand justify-content-start" href="#">Dresscode</a>
  </h4>
  <ul class="nav">
    <li class="nav-item">
      <a class="nav-link" href="#">Home</a>
    </li>
  </ul>
</nav>

<div class="card" style="max-width: 1100px;">
  <div class="row no-gutters">
    <div class="col-md-4">
      <img class="card-img-top"
           src="<%= asset_path @prenda.tipo %>" alt="Card image cap">
    </div>
    <div class="col-md-8">
      <div class="card-body">
        <h5 class="card-title"><%== @prenda.tipo%> </h5>
        <p class="card-text"><%== @prenda.descripcion%></p>
        <p class="card-text"><%== @prenda.cprimario%></p>
        <p class="card-text"><%== @prenda.csecundario%></p>

        <p class="card-text"><small class="text-muted"><%= @prenda.categoria %></small></p>

      </div>
    </div>
  </div>
</div>

